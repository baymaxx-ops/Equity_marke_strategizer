{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb",
                "includeFiles": "public/**"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.py"
        }
    ]
}
