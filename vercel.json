{
  "version": 2,
  "builds": [
    {
      "src": "requirements.txt",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/strategy.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/strategy.py"
    }
  ]
}